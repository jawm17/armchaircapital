         particles.push(\n                    {\n                        element: bubble,\n                        size,\n                        spinVal,\n                        spinSpeed,\n                        speedUp,\n                        top,\n                        left,\n                        z\n                    });\n            }\n        }\n    }\n\n    function moveCoins() {\n        if (document.getElementById(props.element)) {\n            particles.forEach((p) => {\n                // update propeties\n                p.top = p.top + p.speedUp;\n                p.spinVal = p.spinVal + p.spinSpeed;\n\n                // check if particle has gone off screen\n                if (p.top >= height + p.size) {\n                    particles = particles.filter((o) => o !== p);\n                    p.element.remove();\n                }\n                // enter properties\n                // transition: position 0.3s;\n                p.element.setAttribute(\"style\", `\n                position: fixed;\n                top: ${p.top}px;\n                left: ${p.left}px;\n                width: ${p.size}px;\n                heigth: ${p.size}px;\n                transform:rotate(${p.spinVal}deg);\n                zIndex: ${p.z}\n            `);\n            });\n        } else {\n            particles = [];\n        }\n    }\n\n    return (\n        <div>\n        </div>\n    );\n}",{"ruleId":"31","severity":1,"message":"32","line":6,"column":10,"nodeType":"33","messageId":"34","endLine":6,"endColumn":20},{"ruleId":"31","severity":1,"message":"35","line":6,"column":22,"nodeType":"33","messageId":"34","endLine":6,"endColumn":35},{"ruleId":"36","severity":1,"message":"37","line":18,"column":61,"nodeType":"38","messageId":"39","endLine":18,"endColumn":63},{"ruleId":"31","severity":1,"message":"40","line":10,"column":9,"nodeType":"33","messageId":"34","endLine":10,"endColumn":17},{"ruleId":"41","severity":1,"message":"42","line":16,"column":28,"nodeTy