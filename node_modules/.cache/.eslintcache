tion genCoin() {\n        if (document.getElementById(props.element) && pageOpen) {\n            if (particles.length < 8) {\n                const width = document.getElementById(props.element).getBoundingClientRect().width;\n                const top = -80;\n                // const left = document.getElementById(props.element).getBoundingClientRect().left;\n                const size = sizes[Math.floor(Math.random() * sizes.length)];\n                const left = Math.random() * width;\n                const spinSpeed = ((Math.random() * 2)) * (Math.random() <= .5 ? -1 : 1);\n                const spinVal = Math.random() * 360;\n                const bubble = document.createElement(\"img\");\n                const speedUp = speeds[Math.floor(Math.random() * speeds.length)];\n                const z = 50\n                bubble.setAttribute(\"style\", `width: ${size}px; height: ${size}px; top:${80}px;  left:${left}px; position:fixed; zIndex: ${z}`);\n                bubble.setAttribute(\"src\", greenCoin);\n                if (Math.floor(Math.random() * 2) === 1) {\n                    bubble.setAttribute(\"class\", \"bubble\");\n                }\n                document.getElementById(props.element).appendChild(bubble);\n                particles.push(\n                    {\n                        element: bubble,\n                        size,\n                        spinVal,\n                        spinSpeed,\n                        speedUp,\n                        top,\n                        left,\n                        z\n                    });\n            }\n        }\n    }\n\n    function moveCoins() {\n        if (document.getElementById(props.element)) {\n            particles.forEach((p) => {\n                // update propeties\n                p.top = p.top + p.speedUp;\n                p.spinVal = p.spinVal + p.spinSpeed;\n\n                // check if particle has gone off screen\n                if (p.top >= height + p.size) {\n                    particles = particles.filter((o) => o !== p);\n                    p.element.remove();\n                }\n                // enter properties\n                // transition: position 0.3s;\n                p.element.setAttribute(\"style\", `\n                position: fixed;\n                top: ${p.top}px;\n                left: ${p.left}px;\n                width: ${p.size}px;\n                heigth: ${p.size}px;\n                transform:rotate(${p.spinVal}deg);\n                zIndex: ${p.z}\n            `);\n            });\n        } else {\n            particles = [];\n        }\n    }\n\n    return (\n        <div>\n        </div>\n    );\n}",{"ruleId":"30","severity":1,"message":"31","line":6,"column":22,"nodeType":"32","messageId":"33","endLine":6,"endColumn":35},{"ruleId":"34","severity":1,"message":"35","line":32,"column":61,"nodeType":"36","messageId":"37","endLine":32,"endColumn":63},{"ruleId":"30","severity":1,"message":"38","line":10,"column":9,"nodeType":"32","messageId":"33","endLine":10,"endColumn":17},{"ruleId":"39","severity":1,"message":"40","line":16,"column":28,"nodeType":"41","endLine":16,"endColumn":32},{"ruleId":"39","severity":1,"message":"42","line":18,"column":29,"nodeType":"43","endLine":18,"endColumn":31},{"ruleId":"39","severity":1,"message":"44","line":19,"column":28,"nodeType":"45","endLine":21,"endColumn":24},"no-unused-vars","'setContacting' is assigned a value but never used.","Identifier","unusedVar","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","'interval' is assigned a value but never used.","react-hooks/exhaustive-deps","Assignments to the 'pageOpen' variable from inside React Hook useEffect will be lost after each render. To preserve the value over time, store it in a useRef Hook and keep the mutable value in the '.current' property. Otherwise, you can move this variable directly inside useEffect.","Literal","Assignments to the 'particles' variable from inside React Hook 